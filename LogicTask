#include <iostream>
using namespace std;
int Find(char str[])
{
	int count = 0;
	int j = 1;
	for (int i = 0; str[i]; i++)
	{
		if (str[i] == '(')
			count++;
		else
			count--;
		if (count == -1)
			break;
		j++;
	}
	return j;
}
int main()
{
	char str[] = "()()())";
	cout << Find(str) << endl;;
	system("pause");
	return 0;
}
